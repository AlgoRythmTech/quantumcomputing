{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@rythmAI_api_url",
    "VITE_AUTH0_DOMAIN": "@auth0_domain",
    "VITE_AUTH0_CLIENT_ID": "@auth0_client_id"
  }
}
